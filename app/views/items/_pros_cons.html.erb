<div class="col-md-6">
  <h1 class="left"><%=pro_con%></h1>

  <table class="table table-responsive">
    <%= render partial: 'items/item_user', locals: {user: list.user}%>
    <%= render @owner_items %>
  </table>

  <% contributor_items_hash.each do |user, items|%>
    <table class="table table-responsive">
      <%= render partial: 'items/item_user', locals: {user: user}%>
      <%= render items %>
    </table>
  <%end%>
</div>
