<!DOCTYPE html>
<html>
  <head>
    <title><%= full_title(yield(:title)) %></title>
    <%= csrf_meta_tags %>
    <%= render 'layouts/rails_default' %>
    <%= render 'layouts/shim' %>
    <%= favicon_link_tag 'favicon.ico' %>

  </head>

  <body>
    <%= render 'layouts/header' %>
    <div class="container">
      <div class="row">
      <%= render 'layouts/messages' %>

        <%= yield %>
      </div>
      <%= render 'layouts/footer' %>
    </div>
  </body>
</html>
